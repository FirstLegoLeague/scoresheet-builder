        <string id="yes">                           כן</string>
        <string id="no">                            לא</string>

        <string id="M01-name">                      M01 נסיעה בחלל</string>
        <string id="M01-desc">                      <![CDATA[התחילו לגלגל כל מטען בברור במורד הרציף לנסיעה בחלל בכל גלגול העגלה חייבת *להיות עצמאית בזמן בו היא מגיעה לחיבור המסילה הראשון*]]></string>
        <string id="M01-scoring1">                   מטען רכב עבר את חיבור המסילה הראשון</string>
        <string id="M01-scoring2">                   מטען אספקה עבר את חיבור המסילה הראשון</string>
        <string id="M01-scoring3">                   מטען צוות עבר את חיבור המסילה הראשון</string>

        <string id="M02-name">                      M02 מערך לוחות סולריים</string>
        <string id="M02-desc">                      <![CDATA[הלוחות הסולאריים צריכים לנטות כלפיכם או מכם והלאה.]]></string>
        <string id="M02-scoring1">                   שני הלוחות הסולאריים נוטים כלפי אותה זירה</string>
        <string id="M02-scoring2">                   הלוח הסולארי שלכם נוטה לזירה של הקבוצה השנייה</string>

        <string id="M03-name">                      M03 הדפסה בתלת מימד</string>
        <string id="M03-desc">                      <![CDATA[גרמו לפליטה של לבנת ה-4X2 *על ידי הנחת דגימת ליבה של רגולית' במדפסת התלת מימד.]]></string>
        <string id="M03-scoring1">                  2x4 לבנת ה-2X4 נפלטה (ע”י הנחת דגימת ליבה של רגולית’ במדפסת התלת מימד בלבד)</string>
        <string id="M03-scoring2">                  2x4 לבנת ה-2X4 לגמרי באזור כוכב הלכת הצפון מזרחי</string>

        <string id="M04-name">                      M04 חציית מכתשים</string>
        <string id="M04-desc">                      <![CDATA[הרובוט או כל רכב אחר אותו הוא שולח צריך לחצות לגמרי את דגם המכתשים, ע"י נסיעה ישירה מעליו. לא לידו. לא מסביבו.]]></string>
        <string id="M04-scoring1">                  כל החלקים התומכים במשקל של הציוד המשמש למעבר חצו לגמרי בין המגדלים </string>
        <string id="M04-scoring2">                 כל הציוד המשמש למעבר חצה ממזרח למערב, לגמרי מעבר לשער המורד</string>

        <string id="M05-name">                      M05 כריית משאבים</string>
        <string id="M05-desc">                      <![CDATA[הרובוט צריך להשיג את כל דגימות הליבה מתוך דגם אתר הליבה, ואז יש לו מספר אפשרויות מה לעשות איתן, כמתואר כאן ובמשימה M03]]></string>
        <string id="M05-scoring1">                  כל ארבע דגימות הליבה לא נוגעות יותר במוט שהחזיק אותן בתוך דגם אתר הליבה</string>
        <string id="M05-scoring2">                  דגימת הליבה של הגז נוגעת בשטיח ולגמרי בתוך עיגול המטרה של רכב הנחיתה</string>
        <string id="M05-scoring3">                  -או- דגימת הליבה של הגז לגמרי בתוך הבסיס</string>
        <string id="M05-scoring4">                  דגימת הליבה של המים נתמכת ע"י תא גידול המזון בלבד</string>
        <string id="M05-error">                     מיקום דגימת הליבה של הגז</string>

        <string id="M06-name">                      M06 רכיבי תחנת החלל</string>
        <string id="M06-desc">                      <![CDATA[הרובוט צריך להסיר ולחבר רכיבים בין השקעים של אתר הביות.לרכיבים שהוכנסו אסור לגעת בשום דבר מלבד אתר הביות.]]></string>
        <string id="M06-scoring1">                   הרכיב החרוטי לגמרי בתוך הבסיס</string>
        <string id="M06-scoring2">                   הרכיב הגלילי הוכנס לתוך השקע המערבי של אתר הביות</string>
        <string id="M06-scoring3">                   רכיב העגינה הוכנס לתוך השקע המזרחי של אתר הביות</string>

        <string id="M07-name">                      M07 הליכת חלל בחירום</string>
        <string id="M07-desc">                      <![CDATA[הרובוט צריך להכניס את הגוף של גרהרד לתא האוויר.]]></string>
        <string id="M07-scoring">                   האסטרונאוט "ג'רהרד" הוכנס לתוך תא האוויר של אתר הביות</string>
        <string id="M07-scoring1">                  לא</string>
        <string id="M07-scoring2">                  באופן חלקי</string>
        <string id="M07-scoring3">                  לגמרי בתוך</string>

        <string id="M08-name">                      M08 פעילות אירובית</string>
        <string id="M08-desc">                      <![CDATA[הרובוט צריך להזיז שוב ושוב את אחת או שתי הידיות המורכבות על מכשיר הכושר כדי לגרום למחוג להתקדם. (אם המחוג מכסה באופן חלקי את הגבול האחרון של הצבע הכתום או האפור , בחרו בצבע הזה) ]]></string>
        <string id="M08-scoring">                   קצה מחוג דגם הפעילות האירובית בתחום (ע"י הזזת אחת או שתי הידיות המורכבות עליו בלבד):</string>
        <string id="M08-scoring1">                  כלום</string>
        <string id="M08-scoring2">                  אפור</string>
        <string id="M08-scoring3">                  לבן</string>
        <string id="M08-scoring4">                  כתום</string>

        <string id="M09-name">                      M09 תרגילי כוח</string>
        <string id="M09-desc">                      <![CDATA[הרובוט צריך להרים את מוט הכוח לגובה ניקוד.]]></string>
        <string id="M09-scoring">                   מוט הכוח מורם כך שהחור הרביעי בפס השיניים נחשף לפחות באופן חלקי</string>

        <string id="M10-name">                      M10 ייצור מזון</string>
        <string id="M10-desc">                      <![CDATA[הזיזו את ידית הדחיפה למרחק הנכון במהירות הנכונה כדי להגיע לתוך טווח הניקוד הירוק.]]></string>
        <string id="M10-scoring">                   משקולת אפורה נפלה אחרי הצבע הירוק אך לפני הבז' (ע"י הזזת ידית הדחיפה בלבד)</string>

        <string id="M11-name">                      M11 מהירות מילוט</string>
        <string id="M11-desc">                      <![CDATA[הרובוט צריך להכות על משטח הפגיעה חזק מספיק כדי שהחללית לא תיפול חזרה.]]></string>
        <string id="M11-scoring">                   החללית נשארת למעלה (ע"י לחיצה/חבטה במשטח הפגיעה בלבד)</string>

        <string id="M12-name">                      M12 מסלולי לויין</string>
        <string id="M12-desc">                      <![CDATA[הרובוט צריך להזיז לוויין אחד או יותר למסלול החיצוני.]]></string>
        <string id="M12-scoring">                   לווינים על או מעל האזור בין שני הקווים של המסלול החיצוני:</string>

        <string id="M13-name">                      M13 מצפה כוכבים</string>
        <string id="M13-desc">                      <![CDATA[סובבו את מצפה הכוכבים לכיוון המדויק (אם המחוג מכסה באופן חלקי את הגבול האחרון של הצבע הכתום או האפור , בחרו בצבע הזה).]]></string>
        <string id="M13-scoring">                   קצה מחוג מצפה הכוכבים בתחום:</string>
        <string id="M13-scoring1">                  כלום</string>
        <string id="M13-scoring2">                  אפור</string>
        <string id="M13-scoring3">                  לבן</string>
        <string id="M13-scoring4">                  כתום</string>

        <string id="M14-name">                      M14 הטיית מטאורואיד</string>
        <string id="M14-desc">                      <![CDATA[ממערב לקו החופשי, שלחו מטאורואיד עצמאי אחד או שניים ללוכד המטאורואידים.]]></string>
        <string id="M14-scoring1">                  מטאורואידים נוגעים בשטיח ונמצאים בתחום המרכזי:</string>
        <string id="M14-scoring2">                  מטאורואידים נוגעים בשטיח ונמצאים בתחום צדדי כלשהו:</string>
        <string id="M14-error">                     צירוף בלתי אפשרי. יותר מדי מטאורואידים</string>

        <string id="M15-name">                      M15 נחיתת רכב הנחיתה</string>
        <string id="M15-desc">                      <![CDATA[הביאו את רכב הנחיתה לאחת המטרות שלו כשהוא שלם, או לפחות הביאו אותו לבסיס.]]></string>
        <string id="M15-scoring1">                  רכב הנחיתה שלם ונוגע בשטיח</string>
        <string id="M15-scoring2">                  רכב הנחיתה לגמרי בתוך:</string>
        <string id="M15-scoring3">                  כלום</string>
        <string id="M15-scoring4">                  בסיס</string>
        <string id="M15-scoring5">                  באזור כוכב הלכת הצפון מזרחי</string>
        <string id="M15-scoring6">                  בתוך עיגול המטרה</string>

        <string id="penalties-name">                עונשין</string>
        <string id="penalties-desc">                <![CDATA[אתם רשאים להציל את הרובוט שלכם בעזרת הידיים, אבל זה גורם לעונשין הזה.]]></string>
        <string id="penalties-scoring">             דיסקיות עונשין במשולש הדרום מזרחי:</string>

    </strings>

