        <string id="yes">                           Kyllä</string>
        <string id="no">                            Ei</string>

        <string id="M01-name">                      M01 Avaruusmatka</string>
        <string id="M01-desc">                      <![CDATA[*Kukin lähetetty kuljetusalus on selvästi rullaannut alas
ramppia pitkin. Kuljetusalusten tulee * kulkea itsenäisesti ylittäessään radan ensimmäistä jatkosta.]]></string>
        <string id="M01-scoring1">                   Kuuauto- alus rullasi itsenäisesti 1. jatkoksen yli</string>
        <string id="M01-scoring2">                   Tarvike-alus rullasi itsenäisesti 1. jatkoksen yli</string>
        <string id="M01-scoring3">                   Miehistö-alus rullasi itsenäisesti 1. jatkoksen yli</string>

        <string id="M02-name">                      M02 Aurinkopanelien suuntaaminen</string>
        <string id="M02-desc">                      <![CDATA[Paneelit tulee kääntää jommankumman joukkueen suuntaan.]]></string>
        <string id="M02-scoring1">                   Both Aurinkopanelit on käännetty samaan suuntaan</string>
        <string id="M02-scoring2">                   Your Aurinkopaneli on käännetty toisen joukkueen suuntaan</string>

        <string id="M03-name">                      M03 3D Tulostus</string>
        <string id="M03-desc">                      <![CDATA[2x4 palikka tulee saada ulos * asettamalla maaperänäyte 3D tulostimeen.]]></string>
        <string id="M03-scoring1">                  2x4 palikka on saatu ulos asettamalla maaperänäyte 3D tulostimeen</string>
        <string id="M03-scoring2">                  2x4 palikka on kokonaan koillisen planeetan alueella</string>

        <string id="M04-name">                      M04 Kraaterin ylitys</string>
        <string id="M04-desc">                      <![CDATA[Robotti tai sen lähettämä tutkimusalus ylittää kraateri-mallin kokonaan kulkemalla suoraan sen yli. Ei läheltä, ei kiertäen.]]></string>
        <string id="M04-scoring1">                  Kaikki laitteen kantavat rakenteet kulkivat tornien välistä</string>
        <string id="M04-scoring2">                  Ylitys suoritettiin kulkien idästä länteen, kokonaan ohi alas painetun portin</string>

        <string id="M05-name">                      M05 Luonnonvarojen hyödyntäminen</string>
        <string id="M05-desc">                      <![CDATA[Robotin tulee tyhjentää näytteet koepora-mallista. Pisteitä saa myös näytteiden viemisestä oikeille paikoille, Huom. tehtävä M03.]]></string>
        <string id="M05-scoring1">                  Yksikään neljästä näytteestä ei enää kosketa koepora-mallin akselia</string>
        <string id="M05-scoring2">                  Kaasunäyte koskettaa alustaa ja on kokonaan laskeutujan maalialueen sisällä (valk)</string>
        <string id="M05-scoring3">                  -TAI- Kaasunäyte on kokonaan kotipesässä</string>
        <string id="M05-scoring4">                  Vesinäyte koskettaa vain ruoankasvatuskammiota (sin)</string>
        <string id="M05-error">                     Kaasunäytteen paikka</string>

        <string id="M06-name">                      M06 Avaruusaseman moduulit</string>
        <string id="M06-desc">                      <![CDATA[Robotin tulee irroittaa ja kytkeä moduuleja aseman asuinyksikön portteihin. Kytketyt modulit eivät kosketa mitään muuta kuin asuinyksikköä.]]></string>
        <string id="M06-scoring1">                   Kartiomoduuli on kokonaan kotipesässä</string>
        <string id="M06-scoring2">                   Lieriömoduuli on kytketty asuinyksikön lännen puoleiseen porttiin</string>
        <string id="M06-scoring3">                   Telakointimoduuli on siirretty asuinyksikön idän puoleiseen porttiin</string>

        <string id="M07-name">                      M07 Hätätapaus avaruuskävelyllä</string>
        <string id="M07-desc">                      <![CDATA[Robotin tulee auttaa Gerhard avaruusaseman ilmalukon sisälle.]]></string>
        <string id="M07-scoring">                   Astronautti Gerhard on asuinyksikän ilmalukon kammiossa:</string>
        <string id="M07-scoring1">                  Ei</string>
        <string id="M07-scoring2">                  Osittain</string>
        <string id="M07-scoring3">                  Kokonaan</string>

        <string id="M08-name">                      M08 Aerobinen harjoitus</string>
        <string id="M08-desc">                      <![CDATA[Robotin tulee yhtä tai molempia kuntoilulaitteen oranssia kahvaa liikuttamalla siirtää kuntomittarin osoitinta. (Osoitin osittain harmaan tai oranssin rajalla tulkitaan joukkueen eduksi.)]]></string>
        <string id="M08-scoring">                   Osoittimen kärki on siirtynyt (vain laitteen kahvoja käyttämällä):</string>
        <string id="M08-scoring1">                  Ei tulosta</string>
        <string id="M08-scoring2">                  Harmaalle</string>
        <string id="M08-scoring3">                  Valkoiselle</string>
        <string id="M08-scoring4">                  Oranssille</string>

        <string id="M09-name">                      M09 Voimaharjoittelu</string>
        <string id="M09-desc">                      <![CDATA[Robotin tulee nostaa painonnostotanko ylös.]]></string>
        <string id="M09-scoring">                   Tanko on nostettu niin ylös, että hammastangon neljän reiän ryhmän alinkin reikä on vähintään osittain näkyvissä.</string>

        <string id="M10-name">                      M10 Ruoan tuottaminen</string>
        <string id="M10-desc">                      <![CDATA[Robotti pyöräyttää laitteen kasvualustat haluttuun asentoon oranssia työnnintä liikuttamalla.]]></string>
        <string id="M10-scoring">                   Harmaa punnus on kokonaan alhaalla ja vihreä kasvatusalusta on pyöräytetty päälimmäiseksi (vain työnnintä liikuttamalla.)</string>

        <string id="M11-name">                      M11 Lähtökiihdytys</string>
        <string id="M11-desc">                      <![CDATA[Robotti saattaa raketin matkaan sellaisella voimalla ettei se putoa takaisin alas.]]></string>
        <string id="M11-scoring">                   Avaruusalus on saatu ylös(vain painamalla/iskemällä laukaisulevyä)</string>

        <string id="M12-name">                      M12 Satelliitit kiertoradalle</string>
        <string id="M12-desc">                      <![CDATA[Robotin tulee kuljettaa yksi tai useampia satelliitteja uloimmalle kiertoradalle.]]></string>
        <string id="M12-scoring">                   Satelliitteja osin tai kokonaan uloimman kiertoradan alueella:</string>

        <string id="M13-name">                      M13 Observatorio</string>
        <string id="M13-desc">                      <![CDATA[Observatorion kääntäminen oikeaan suuntaan. (Osoitin osittain harmaan tai oranssin rajalla tulkitaan joukkueen eduksi.)]]></string>
        <string id="M13-scoring">                   Osoittimen kärki osoittaa:</string>
        <string id="M13-scoring1">                  Ei tulosta</string>
        <string id="M13-scoring2">                  Harmaa</string>
        <string id="M13-scoring3">                  Valkoinen</string>
        <string id="M13-scoring4">                  Oranssi</string>

        <string id="M14-name">                      M14 Meteoriitin poikkeuttaminen radaltaan</string>
        <string id="M14-desc">                      <![CDATA[1-2 meteoriittia lähetetään Vapaa-linjan länsi-puolelta kulkemaan itsenäisesti meteoriitti-pyydykseen.]]></string>
        <string id="M14-scoring1">                  Meteoriitteja koskettaa alustaa pyydyksen keski-lokerossa:</string>
        <string id="M14-scoring2">                  Meteoriitteja koskettaa alustaa pyydyksen sivu-lokeroissa:</string>
        <string id="M14-error">                     Mahdoton tulos. Liian monta Meteoriittia</string>

        <string id="M15-name">                      M15 Turvallinen laskeutuminen</string>
        <string id="M15-desc">                      <![CDATA[Laskeutuja tulee avustaa maalialueelleen, tai tuoda turvallisesti kotipesään.]]></string>
        <string id="M15-scoring1">                  Laskeutuja on ehjä ja koskettaa alustaa</string>
        <string id="M15-scoring2">                  Laskeutuja on kokonaan (sisällä):</string>
        <string id="M15-scoring3">                  Ei tulosta</string>
        <string id="M15-scoring4">                  Kotipesässä</string>
        <string id="M15-scoring5">                  Koillisen planeetan alueella</string>
        <string id="M15-scoring6">                  Maalialueellaan</string>

        <string id="penalties-name">                Rangaistukset</string>
        <string id="penalties-desc">                <![CDATA[Teillä on lupa pelastaa robotti, mutta siitä seuraa rangaistus]]></string>
        <string id="penalties-scoring">             Rangaistuksia kertyi erän aikana kpl:</string>

    </strings>

