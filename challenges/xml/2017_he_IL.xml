    <strings rtlDoc="true">

        <string id="yes">                           כן</string>
        <string id="no">                            לא</string>

        <string id="M01-name">                      M01 - הסרת צינור</string>
        <string id="M01-desc">                      <![CDATA[הזיזו את הצינור הפגום לתוך הבסיס]]></string>
        <string id="M01-scoring">                   צינור פגום לגמרי בתוך הבסיס</string>

        <string id="M02-name">                      M02 - זרימה</string>
        <string id="M02-desc">                      <![CDATA[הזיזו מים גדולים (אחד לכל היותר) אל המגרש של הקבוצה השניה *על ידי סיבוב הידיות של המערכת בלבד*.]]></string>
        <string id="M02-scoring">                   מים גדולים עברו לזירה של הקבוצה השנייה
(רק על ידי סיבוב שסתום (שסתומי) מערכת השאיבה) </string>

        <string id="M03-name">                      M03 - הרחבת משאבה</string>
        <string id="M03-desc">                      <![CDATA[הזיזו את הרחבת המשאבה כך שתיגע במגרש וכך שמגע זה הוא לגמרי בתוך אזור המטרה]]></string>
        <string id="M03-scoring">                   הרחבת המשאבה במגע עם השטיח, לגמרי בתוך באזור המטרה</string>

        <string id="M04-name">                      M04 - גשם</string>
        <string id="M04-desc">                      <![CDATA[גרמו לכך שלפחות גשם אחד יצא מענן הגשם]]></string>
        <string id="M04-scoring">                   לפחות גשם אחד יצא מתוך ענן הגשם</string>

        <string id="M05-name">                      M05 - מסנן</string>
        <string id="M05-desc">                      <![CDATA[הזיזו את המסנן צפונה עד שוו המנעול נופל.]]></string>
        <string id="M05-scoring">                   תפס הנעילה נפל ונשאר שם</string>

        <string id="M06-name">                      M06 - טיפול במים</string>
        <string id="M06-desc">                      <![CDATA[ הביאו לכך שמתקן טיפול המים יפלוט מים גדולים *רק באמצעות הורדת ידית האסלה. ]]></string>
        <string id="M06-scoring">                   מים גדולים נפלטו מדגם טיפול המים (רק על ידי ידית בית השימוש)</string>

        <string id="M07-name">                      M07 - מזרקה</string>
        <string id="M07-desc">                      <![CDATA[הביאו לכך שהשכבה האמצעית של המזרקה עולה באופן ברור ונשארת בגובה זה, אך ורק בעקבות מים גדולים באמבט האפור.]]></string>
        <string id="M07-scoring">                   השכבה האמצעית עלתה (רק בשל דגם מים גדולים בתוך האמבט האפור)</string>

        <string id="M08-name">                      M08 - מכסי ביוב</string>
        <string id="M08-desc">                      <![CDATA[הפכו מכסי ביוב מבלי שיגיעו לבסיס.]]></string>
        <string id="M08-scoring1">                  מכסה (מכסי) ביוב הפוכים בברור מעבר למצב זקוף (ומעולם לא הגיעו לבסיס)</string>
        <string id="M08-scoring2">                  שני המכסים הפוכים</string>
        <string id="M08-scoring3">                  שני המכסים לגמרי בתוך אזורי מטרה נפרדים של החצובה</string>

        <string id="M09-name">                      M09 - חצובה</string>
        <string id="M09-desc">                      <![CDATA[הזיז את חצובת המצלמה.]]></string>
        <string id="M09-scoring1">                  כל רגלי החצובה נוגעות בשטיח</string>
        <string id="M09-scoring2">                  החצובה באופן חלקי באזור המטרה שלה</string>
        <string id="M09-scoring3">                  החצובה לגמרי בתוך אזור המטרה שלה</string>
        <string id="M09-error">                     יש לבחור אחת מבין שתי האפשרויות</string>

        <string id="M10-name">                      M10 - החלפת צינור</string>
        <string id="M10-desc">                      <![CDATA[הזיזו צינור חדש כך שהוא יהיה במקום הצינור הפגום, במגע מלא/אופקי עם השטיח.]]></string>
        <string id="M10-scoring1">                  צינור חדש נמצא היכן שהצינור הפגום היה </string>
        <string id="M10-scoring2">                  הצינור החדש במגע מלא/ אופקי עם השטיח</string>

        <string id="M11-name">                      M11 - הנחת צינור</string>
        <string id="M11-desc">                      <![CDATA[הזיזו צינור חדש.]]></string>
        <string id="M11-scoring1">                  הצינור החדש במגע מלא/אופקי עם השטיח</string>
        <string id="M11-scoring2">                  הצינור החדש באופן חלקי באזור המטרה שלו</string>
        <string id="M11-scoring3">                  הצינור החדש לגמרי בתוך אזור המטרה שלו</string>
        <string id="M11-error">                     יש לבחור אחת מבין שתי האפשרויות</string>

        <string id="M12-name">                      M12 - בוצה</string>
        <string id="M12-desc">                      <![CDATA[הזיזו את הבוצה כך שהיא תיגע בעץ הגלוי של אחד מבין ששת האדניות המצוירות.]]></string>
        <string id="M12-scoring">                   הבוצה נוגעת בעץ הגלוי של דופן אדנית מצויירת</string>

        <string id="M13-name">                      M13 - פרח</string>
        <string id="M13-desc">                      <![CDATA[גרמו לפרח לעלות באופן ברור, כך שישאר שם אך ורק בעקבות מים גדולים בכד החום.]]></string>
        <string id="M13-scoring1">                  הפרח עלה (רק בשל דגם מים גדולים בתוך הכד החום)</string>
        <string id="M13-scoring2">                  לפחות גשם אחד בחלק הסגול, נוגע בדגם הפרח בלבד</string>

        <string id="M14-name">                      M14 - באר מים</string>
        <string id="M14-desc">                      <![CDATA[הזיזו את באר המים כך שתהיה במגע עם השטיח.]]></string>
        <string id="M14-scoring1">                  באר המים במגע עם השטיח באופן חלקי באזור המטרה</string>
        <string id="M14-scoring2">                  באר המים במגע עם השטיח לגמרי בתוך אזור המטרה</string>

        <string id="M15-name">                      M15 - אש</string>
        <string id="M15-desc">                      <![CDATA[גרמו לאש ליפול רק על ידי כך שהכבאית הפעילה כוח ישיר על מנוף הבית.]]></string>
        <string id="M15-scoring">                   האש נפלה ונשארה שם (רק מפני שהכבאית הפעילה כוח ישיר על מנוף הבית)</string>
                                                                                            
        <string id="M16-name">                      M16 - איסוף מים</string>
        <string id="M16-desc">                      <![CDATA[הזיזו או תפסו מים גדולים ו/או מי גשם (אחד לכל היותר) כך שהוא נוגע בשטיח בתוך המטרה, *מבלי שהמטרה תגיע לקו התיחום הלבן. למים מותר לגעת במטרה או במים אחרים, אך לא להיתמך או לגעת בשום דבר אחר. כל מים מקנה נקודות בנפרד]]></string>
        <string id="M16-scoring1">                  המטרה של המים מזרחית לקו התיחום הלבן (ומעולם לא הגיעה לקו התיחום הלבן)</string>
        <string id="M16-scoring2">                  לפחות גשם אחד נוגע בשטיח בתוך המטרה של המים</string>
        <string id="M16-scoring3">                  מים גדולים נוגעים בשטיח בתוך המטרה של המים</string>
        <string id="M16-scoring4">                  לפחות זוג אחד של מים גדולים בערימה בתוך המטרה של המים</string>

        <string id="M17-name">                      M17 - מערכת טיהור מים - SLINGSHOT &#8238;</string>
        <string id="M17-desc">                      <![CDATA[הזיזו את ה SLINGSHOT כך שהוא לגמרי בתוך אזור המטרה שלו.]]></string>
        <string id="M17-scoring1">                  ה-SLINGSHOT לגמרי בתוך אזור המטרה שלו</string>
        <string id="M17-scoring2">                  גשם ומים עכורים לגמרי בתוך אזור המטרה של ה- SLINGSHOT</string>

        <string id="M18-name">                      M18 - ברז</string>
        <string id="M18-desc">                      <![CDATA[גרמו למפלס המים להיות בבירור יותר כחול מלבן, במבט מעל הכוס, *רק על ידי סיבוב ידית הברז.]]></string>
        <string id="M18-scoring">                   מפלס המים כחול יותר מאשר לבן (רק על ידי סיבוב ידית הברז)</string>

        <string id="penalties-name">                עונשין</string>
        <string id="penalties-desc">                <![CDATA[עונשין שניתן]]></string>
        <string id="penalties-scoring">             דיסקיות עונשין בתוך אזור המשולש הלבן</string>

    </strings>

